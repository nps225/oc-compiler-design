#include "oclib.h"

int main() {
   int number = 1;
   int done = FALSE;
   while (not done) {
      putint (number);
      putchr ('\n');
      if (number <= 0) done = TRUE;
      number = number + number;
   }
   return EXIT_SUCCESS;
}

